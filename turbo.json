{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"globalEnv": [
		"BETTER_AUTH_SECRET",
		"DATABASE_URL",
		"BLOB_READ_WRITE_TOKEN",
		"BETTER_AUTH_URL",
		"NODE_ENV"
	],
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": ["dist/**", ".next/**", "!.next/cache/**"],
			"env": [
				"BETTER_AUTH_SECRET",
				"DATABASE_URL",
				"BLOB_READ_WRITE_TOKEN",
				"BETTER_AUTH_URL",
				"NODE_ENV"
			]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"dev": {
			"cache": false,
			"persistent": true,
			"env": [
				"BETTER_AUTH_SECRET",
				"DATABASE_URL",
				"BLOB_READ_WRITE_TOKEN",
				"BETTER_AUTH_URL",
				"NODE_ENV"
			]
		},
		"db:push": {
			"cache": false,
			"persistent": true,
			"env": [
				"DATABASE_URL"
			]
		},
		"db:studio": {
			"cache": false,
			"persistent": true,
			"env": [
				"DATABASE_URL"
			]
		},
		"db:migrate": {
			"cache": false,
			"persistent": true,
			"env": [
				"DATABASE_URL"
			]
		},
		"db:generate": {
			"cache": false,
			"persistent": true,
			"env": [
				"DATABASE_URL"
			]
		}
	}
}